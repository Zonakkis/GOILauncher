<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:Width="auto" d:Height="auto"
        xmlns:vm="using:GOI地图管理器.ViewModels"
        xmlns:views="using:GOI地图管理器.Views"
        x:Class="GOI地图管理器.Views.MessageBoxWindow"
		x:DataType="vm:MessageBoxWindowViewModel"
        Title="提示"
		WindowStartupLocation="CenterOwner"
		SizeToContent="WidthAndHeight">
	<Grid RowDefinitions="auto,auto">
		<TextBlock Grid.Row="0" Text="{Binding Message}" HorizontalAlignment="Center" Margin="10,10"/>
		<Button Grid.Row="1" Content="确定" Command="{Binding CloseCommand}" HorizontalAlignment="Center" Margin="10,10"/>
	</Grid>
</Window>
